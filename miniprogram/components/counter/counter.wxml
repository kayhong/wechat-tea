<!--miniprogram/components/counter/counter.wxml-->
<!-- 主容器 -->
<view class="stepper">
	<!-- 减号 -->
	<text class="normal" bindtap="bindMinus">-</text>
	<!-- 数值 -->
	<input type="number" bindchange="bindManual" value="{{num}}" />
	<!-- 加号 -->
	<text class="normal" bindtap="bindPlus">+</text>
</view>
